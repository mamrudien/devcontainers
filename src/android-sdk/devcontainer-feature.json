{
    "name": "Android SDK",
    "id": "android-sdk-feature",
    "version": "0.1.0",
    "description": "Providing the necessary tools and environment for Android development.",
    "containerEnv": {
      "ANDROID_HOME": "/usr/local/android/sdk",
      "PATH": "/usr/local/android/sdk/cmdline-tools:/usr/local/android/sdk/cmdline-tools/bin:${PATH}"
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}
